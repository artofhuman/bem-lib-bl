block b-pagination {
    content: [
        {
            elem: 'list',
            tag: 'ul',
            content: [
                {
                    elem: 'arrow',
                    tag: 'li',
                    content: {
                        block: 'b-link',
                        url: '#',
                        content: {
                            block: 'b-icon',
                            mods: { action: 'first-page'}
                        }
                    }
                },
                {
                    elem: 'arrow',
                    tag: 'li',
                    content: {
                        block: 'b-link',
                        url: '#',
                        content: {
                            block: 'b-icon',
                            mods: { action: 'prev-page'}
                        }
                    }
                },
                {
                    elem: 'page',
                    tag: 'li',
                    content: {
                        block: 'b-link',
                        url: '#',
