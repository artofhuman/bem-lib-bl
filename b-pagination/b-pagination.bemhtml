                },
                {
                    elem: 'page',
                    tag: 'li',
                    content: {
                        block: 'b-link',
                        url: '#',
                        content: '5'
                    }
                },
                {
                    elem: 'arrow',
                    tag: 'li',
                    content: {
                        block: 'b-link',
                        url: '#',
                        content: {
                            block: 'b-icon',
                            mods: { action: 'next-page'}
                        }
                    }
                },
                {
                    elem: 'arrow',
                    tag: 'li',
                    content: {
                        block: 'b-link',
                        url: '#',
                        content: {
                            block: 'b-icon',
                            mods: { action: 'last-page'}
                        }
                    }
                }
                
            ]
        }
    ]
}