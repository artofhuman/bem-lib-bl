                    }
                },
                {
                    elem: 'page',
                    tag: 'li',
                    content: {
                        block: 'b-link',
                        url: '#',
                        content: '2'
                    }
                },
                {
                    elem: 'page',
                    tag: 'li',
                    mods: { state: 'current' },
